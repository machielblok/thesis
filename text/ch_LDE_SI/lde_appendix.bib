@article{2011ApPhL..98m3107M,
author = {Marseglia, L. and Hadden, J. P. and Stanley-Clarke, A. C. and Harrison, J. P. and Patton, B. and Ho, Y-L. D. and Naydenov, B. and Jelezko, F and Meijer, J. and Dolan, P. R. and Smith, J. M. and Rarity, J. G. and O'Brien, J. L.},
title = {{Nanofabricated solid immersion lenses registered to single emitters in diamond}},
journal = {Appl. Phys. Lett.},
year = {2011},
volume = {98},
number = {1},
pages = {3107},
month = mar,
affiliation = {Department of Electrical and Electronic Engineering, H. H. Wills Physics Laboratory, Centre for Quantum Photonics, University of Bristol, Bristol BS8 1UB, United Kingdom},
doi = {10.1063/1.3573870},
rating = {0},
date-added = {2011-06-10T13:38:13GMT},
date-modified = {2012-12-12T10:31:55GMT},
abstract = {We describe a technique for fabricating micro- and nanostructures incorporating fluorescent defects in diamond with a positional accuracy better than hundreds of nanometers. Using confocal fluorescence microscopy and focused ion beam etching, we initially locate a suitable defect with respect to registration marks on the diamond surface then etch a structure using these coordinates. We demonstrate the technique by etching an 8 μm diameter hemisphere positioned with single negatively charged nitrogen-vacancy defect lies at its origin. Direct comparison of the fluorescence photon count rate before and after fabrication shows an eightfold increase due to the presence of the hemisphere.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2011ApPhL..98m3107M&link_type=ABSTRACT},
local-url = {file://localhost/Users/wp/Library/Application%20Support/Papers2/Files/Appl.%20Phys.%20Lett.%202011%20Marseglia.pdf},
file = {{Appl. Phys. Lett. 2011 Marseglia.pdf:/Users/wp/Library/Application Support/Papers2/Files/Appl. Phys. Lett. 2011 Marseglia.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1063/1.3573870}}
}

@article{Childress2006,
author = {Childress, L. and Gurudev Dutt, M V and Taylor, J M and Zibrov, A. S. and Jelezko, F and Wrachtrup, J and Hemmer, P. R. and Lukin, M. D.},
title = {{Coherent dynamics of coupled electron and nuclear spin qubits in diamond.}},
journal = {Science},
year = {2006},
volume = {314},
number = {5797},
pages = {281--285},
doi = {10.1126/science.1131871},
pmid = {16973839},
read = {Yes},
rating = {0},
date-added = {2011-06-10T13:38:13GMT},
date-modified = {2012-12-12T10:31:55GMT},
abstract = {Understanding and controlling the complex environment of solid-state quantum bits is a central challenge in spintronics and quantum information science. Coherent manipulation of an individual electron spin associated with a nitrogen-vacancy center in diamond was used to gain insight into its local environment. We show that this environment is effectively separated into a set of individual proximal 13C nuclear spins, which are coupled coherently to the electron spin, and the remainder of the 13C nuclear spins, which cause the loss of coherence. The proximal nuclear spins can be addressed and coupled individually because of quantum back-action from the electron, which modifies their energy levels and magnetic moments, effectively distinguishing them from the rest of the nuclei. These results open the door to coherent manipulation of individual isolated nuclear spins in a solid-state environment even at room temperature.},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16973839},
local-url = {file://localhost/Users/wp/Library/Application%20Support/Papers2/Files/Science%202006%20Childress.pdf},
file = {{Science 2006 Childress.pdf:/Users/wp/Library/Application Support/Papers2/Files/Science 2006 Childress.pdf:application/pdf;Science 2006 Childress.pdf:/Users/wp/Library/Application Support/Papers2/Files/Science 2006 Childress.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1126/science.1131871}}
}

@article{2010ApPhL..97x1901H,
author = {Hadden, J. P. and Harrison, J. P. and Stanley-Clarke, A. C. and Marseglia, L. and Ho, Y-L. D. and Patton, B. R. and O'Brien, J. L. and Rarity, J. G.},
title = {{Strongly enhanced photon collection from diamond defect centers under microfabricated integrated solid immersion lenses}},
journal = {Appl. Phys. Lett.},
year = {2010},
volume = {97},
number = {2},
pages = {1901},
month = dec,
affiliation = {Centre for Quantum Photonics, Department of Electrical and Electronic Engineering {\&} H. H. Wills Physics Laboratory, University of Bristol, Merchant Venturers Building, Woodland Road, Bristol BS8 1UB, United Kingdom},
doi = {10.1063/1.3519847},
rating = {0},
date-added = {2011-06-10T13:38:13GMT},
date-modified = {2012-12-12T10:31:55GMT},
abstract = {The efficiency of photon collection from optically active defect centers in bulk diamond is greatly reduced by refraction at the diamond-air interface. We report on the fabrication and measurement of a geometrical solution to the problem; integrated solid immersion lenses (SILs) etched directly into the surface of diamond. An increase of a factor of 10 was observed in the saturated count-rate from a single negatively charged nitrogen-vacancy (NV-) within a 5 μm diameter SIL compared with NV-'s under a planar surface in the same crystal. Such a system is potentially scalable and easily adaptable to other defect centers in bulk diamond.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2010ApPhL..97x1901H&link_type=ABSTRACT},
local-url = {file://localhost/Users/wp/Library/Application%20Support/Papers2/Files/Appl.%20Phys.%20Lett.%202010%20Hadden.pdf},
file = {{Appl. Phys. Lett. 2010 Hadden.pdf:/Users/wp/Library/Application Support/Papers2/Files/Appl. Phys. Lett. 2010 Hadden.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1063/1.3519847}}
}

