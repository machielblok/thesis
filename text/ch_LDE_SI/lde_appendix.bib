@article{2012PhRvL.108d3604B,
author = {Bernien, Hannes and Childress, Lilian and Robledo, Lucio and Markham, Matthew and Twitchen, Daniel and Hanson, Ronald},
title = {{Two-Photon Quantum Interference from Separate Nitrogen Vacancy Centers in Diamond}},
journal = {Phys. Rev. Lett.},
year = {2012},
volume = {108},
number = {4},
pages = {043604},
month = jan,
affiliation = {Kavli Institute of Nanoscience Delft, Delft University of Technology, PO Box 5046, 2600 GA Delft, The Netherlands},
doi = {10.1103/PhysRevLett.108.043604},
read = {Yes},
rating = {0},
date-added = {2012-01-12T16:38:04GMT},
date-modified = {2012-12-12T10:31:55GMT},
abstract = {We report on the observation of quantum interference of the emission from two separate nitrogen vacancy (NV) centers in diamond. Taking advantage of optically induced spin polarization in combination with polarization filtering, we isolate a single transition within the zero-phonon line of the nonresonantly excited NV centers. The time-resolved two-photon interference contrast of this filtered emission reaches 66%. Furthermore, we observe quantum interference from dissimilar NV centers tuned into resonance through the dc Stark effect. These results pave the way towards measurement-based entanglement between remote NV centers and the realization of quantum networks with solid-state spins.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2012PhRvL.108d3604B&link_type=ABSTRACT},
local-url = {file://localhost/Users/wp/Library/Application%20Support/Papers2/Files/Phys.%20Rev.%20Lett.%202012%20Bernien-2.pdf},
file = {{Phys. Rev. Lett. 2012 Bernien-2.pdf:/Users/wp/Library/Application Support/Papers2/Files/Phys. Rev. Lett. 2012 Bernien-2.pdf:application/pdf;Phys. Rev. Lett. 2012 Bernien-2.pdf:/Users/wp/Library/Application Support/Papers2/Files/Phys. Rev. Lett. 2012 Bernien-2.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1103/PhysRevLett.108.043604}}
}

